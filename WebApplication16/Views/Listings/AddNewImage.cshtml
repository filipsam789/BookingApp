@model WebApplication16.Models.AddNewImageViewModel
@{
    ViewBag.Title = "Add New Image";
}

<h3>Add new image url for listing</h3>

@using (Html.BeginForm("InsertNewImage", "Listings"))
{
    <div class="form-group">
        @Html.EditorFor(m => m.Image, new { @class = "form-control" })
        @Html.HiddenFor(m => m.IdListing)
    </div>
    <div>
        <button type="submit" class="btn btn-default">Add image</button>
    </div>
}
