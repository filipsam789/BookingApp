@model WebApplication16.Models.AddNewPhoneViewModel
@{
    ViewBag.Title = "Add New Phone Number";
}

<h3>Add new phone number for the property @Model.Property.Name</h3>

@using (Html.BeginForm("InsertNewPhoneNumber", "AllProperties"))
{
    <div class="form-group">
        @Html.EditorFor(m => m.PhoneNumber, new { @class = "form-control" })
        @Html.HiddenFor(m => m.IdProperty)
    </div>
    <div>
        <button type="submit" class="btn btn-default">Add phone number</button>
    </div>
}
